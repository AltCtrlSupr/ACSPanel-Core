<Directory /var/www/symfony2-site-zaspanel-static/web/>
    Options -Indexes +Includes
    XBitHack on
    AllowOverride None

    AddOutputFilter DEFLATE css

    Header unset ETag
    FileETag None

    ExpiresActive on
    # cache common graphics for 3 days
    ExpiresByType image/jpg "access plus 3 days"
    ExpiresByType image/gif "access plus 3 days"
    ExpiresByType image/jpeg "access plus 3 days"
    ExpiresByType image/png "access plus 3 days"

    # Can afford to do this because we change the css name each time it changes:
    ExpiresByType text/css "access plus 7 days"

    ExpiresDefault "access plus 1 hours"

    __HTPASSWD__

</Directory>

Include /etc/packaged-site/symfony2-site-zaspanel-static/appapachesettings
