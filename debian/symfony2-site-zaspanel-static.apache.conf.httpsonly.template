<VirtualHost __PRIMARYVIRTUALHOST__>
    ServerName __SERVERNAME__
    __ALIASES__

    SSLEngine on
    SSLCertificateFile    /etc/apache2/ssl/symfony2-site-zaspanel-static.crt
    SSLCertificateKeyFile /etc/apache2/ssl/symfony2-site-zaspanel-static.key
    SSLCertificateChainFile /etc/apache2/ssl/symfony2-site-zaspanel-static.chain

    DocumentRoot /var/www/symfony2-site-zaspanel-static/web/

    CustomLog /var/log/sitelogs/symfony2-site-zaspanel-static/access.log combined
    ErrorLog  /var/log/sitelogs/symfony2-site-zaspanel-static/error.log

    Include /etc/packaged-site/symfony2-site-zaspanel-static/apachedir
</VirtualHost>
