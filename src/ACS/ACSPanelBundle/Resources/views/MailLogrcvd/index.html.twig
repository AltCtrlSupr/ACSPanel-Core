{% extends '::base.html.twig' %}
{% block body %}

<h1>MailLogrcvd list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Sender</th>
            <th>Rcpt</th>
            <th>Enabled</th>
            <th>Createdat</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('maillogrcvd_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.sender }}</td>
            <td>{{ entity.rcpt }}</td>
            <td>{{ entity.enabled }}</td>
            <td>{% if entity.createdAt %}{{ entity.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('maillogrcvd_show', { 'id': entity.id }) }}">show</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% endblock %}
