{% extends '::base.html.twig' %}
{% block header_title %} - {% trans %}DnsRecord creation{% endtrans %}{% endblock %}
{% block body %}
<h1>{%trans%}DnsRecord creation{%endtrans%}</h1>

<div class="form_box">
    <form action="{{ path('dnsrecord_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit">{%trans%}Create{%endtrans%}</button>
        </p>
    </form>
</div>

<ul class="record_actions">
    <li>
        <a href="{{ path('dnsrecord') }}">
            {%trans%}Back to the list{%endtrans%}
        </a>
    </li>
</ul>

{% endblock %}
