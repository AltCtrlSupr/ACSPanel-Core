{% extends '::base.html.twig' %}
{% block header_title %} - {% trans %}MailMailbox{% endtrans %}{% endblock %}
{% block body %}

<table class="record_properties table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ entity.password }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Maildir</th>
            <td>{{ entity.maildir }}</td>
        </tr>
        <tr>
            <th>Quotalimit</th>
            <td>{{ entity.quotaLimit }}</td>
        </tr>
        <tr>
            <th>Usedquota</th>
            <td>{{ entity.usedQuota }}</td>
        </tr>
        <tr>
            <th>Bytes</th>
            <td>{{ entity.bytes }}</td>
        </tr>
        <tr>
            <th>Messages</th>
            <td>{{ entity.messages }}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updatedat</th>
            <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('mailmailbox') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('mailmailbox_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('mailmailbox_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
